    "$schema": "https://swc.rs/schema.json",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true
        },
        "target": "es2018",
        "paths": {
            "@/*": ["./src/*"]
        },
        "baseUrl": "."
    },
    "module": {
        "type": "commonjs"
    }
}
